{
    "version": 2,
    "builds": [
      {
        "src": "api/download.py",
        "use": "@vercel/python"
      },
      {
        "src": "public/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/download",
        "dest": "/api/download.py"
      },
      {
        "src": "/(.*)",
        "dest": "/public/index.html"
      }
    ]
  }
  